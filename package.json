{
  "name": "abbie",
  "version": "1.1.9",
  "description": "A simple, clean, javascript logger",
  "main": "dist/index.js",
  "author": "jamesinaxx",
  "license": "MIT",
  "scripts": {
    "build": "node build/index.js",
    "pretest": "yarn build",
    "test": "yarn pretest && yarn test:js && yarn test:ts",
    "test:js": "node tests/test.js",
    "test:ts": "npx ts-node tests/test.ts",
    "coverage": "npx nyc yarn test && npx nyc report --reporter=text-lcov > coverage.lcov && npx codecov",
    "ci-build": "set e && yarn build && npm run coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jamesinaxx/Abbie.git"
  },
  "keywords": [
    "logger",
    "simple",
    "clean"
  ],
  "bugs": {
    "url": "https://github.com/jamesinaxx/Abbie/issues"
  },
  "homepage": "https://github.com/jamesinaxx/Abbie",
  "devDependencies": {
    "@types/node": "12.20.21",
    "@vercel/ncc": "^0.29.2",
    "eslint": "^7.32.0",
    "fs-extra": "^10.0.0",
    "prettier": "^2.3.2",
    "typescript": "4.3.5"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  }
}
